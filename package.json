{
  "name": "rca-agent",
  "version": "0.1.0",
  "description": "Root Cause Analysis Agent for Kotlin/Android Debugging",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:accuracy": "ts-node scripts/run-accuracy-tests.ts",
    "test:golden": "RUN_GOLDEN_TESTS=true jest tests/golden",
    "perf-test": "ts-node scripts/run-performance-tests.ts",
    "perf-test:quick": "ts-node scripts/run-performance-tests.ts -- --complexity simple",
    "perf-compare": "ts-node scripts/performance-comparison.ts",
    "perf-validate": "ts-node scripts/validate-setup.ts",
    "bench": "ts-node scripts/benchmark.ts",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "debugging",
    "llm",
    "kotlin",
    "android",
    "rca"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@types/uuid": "^10.0.0",
    "chromadb": "^3.1.8",
    "node-fetch": "^3.3.0",
    "uuid": "^13.0.0",
    "zod": "^3.22.4"
  }
}
